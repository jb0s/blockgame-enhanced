	"text.autoconfig.blockgameenhanced.option.partyHudConfig.markNotify": "Play marker sounds",
	"text.autoconfig.blockgameenhanced.option.partyHudConfig.markNotify.@Tooltip": "Enable sound that plays when a party member marks a location.",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableAutoRespawn": "Automatically respawn",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableAutoRespawn.@Tooltip": "If turned on, will automatically respawn you shortly after dying.\nNOTE: Disabling this option will currently disable the custom game-over sequence.\nThis is not intended and will be resolved in a future release.",
	"text.autoconfig.blockgameenhanced.option.partyHudConfig.outlineMembers": "Outline party members",
	"text.autoconfig.blockgameenhanced.option.partyHudConfig.outlineMembers.@Tooltip": "Draws a blue outline around party members when enabled\nso that you can easily distinguish them from other players\nin the world.",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableCustomTitleScreen": "Enable custom Title Screen",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableCustomTitleScreen.@Tooltip": "Overrides the Minecraft title screen with a custom Blockgame one when enabled.",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableItemLabels": "Enable item labels",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableItemLabels.@Tooltip": "Displays item names and their amount above items on the floor when enabled.\nNOTE: Relog for changes to fully take effect.",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableUpdateChecker": "Check for updates",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableUpdateChecker.@Tooltip": "Checks for mod updates on startup when enabled.",
	"text.autoconfig.blockgameenhanced.option.privacyConfig.enableDiscordRpc": "Enable Discord RPC",
	"text.autoconfig.blockgameenhanced.option.privacyConfig.enableDiscordRpc.@Tooltip": "Announce to all your friends that you play game. It's the way.",
	"text.autoconfig.blockgameenhanced.option.privacyConfig.discordRpcShowIngameStatus": "Discord RPC: Display in-game status",
	"text.autoconfig.blockgameenhanced.option.privacyConfig.discordRpcShowIngameStatus.@Tooltip": "Whether or not to display if you are logged in on your Discord Rich Presence.",
	"text.autoconfig.blockgameenhanced.option.privacyConfig.discordRpcShowPartyStatus": "Discord RPC: Display party status",
	"text.autoconfig.blockgameenhanced.option.privacyConfig.discordRpcShowPartyStatus.@Tooltip": "Whether or not to display stats about an ongoing party on your Discord Rich Presence.",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableLootAllButton": "Enable Loot All button",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableLootAllButton.@Tooltip": "Enables/disables the 'Loot All' button that appears in Chest / Barrel interfaces.",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableAutofillDeposit": "Enable Deposit autofill",
	"text.autoconfig.blockgameenhanced.option.accessibilityConfig.enableAutofillDeposit.@Tooltip": "Whether or not to automatically put all your currency in the deposit window upon opening.\n(Does not automatically deposit, only fills)",
