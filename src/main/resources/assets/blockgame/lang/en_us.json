	"text.autoconfig.blockgameenhanced.option.partyHudConfig.showHud": "Show party member view",
	"text.autoconfig.blockgameenhanced.option.partyHudConfig.showHud.@Tooltip": "If disabled, the list of party members will no longer appear on-screen.",
	"text.autoconfig.blockgameenhanced.option.partyHudConfig.showSelf": "Display yourself in party list",
